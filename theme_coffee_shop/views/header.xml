<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    Navbar-->
    <template id="coffee_shop_header" inherit_id="website.template_header_default">
        <xpath expr="//t[@t-call='website.placeholder_header_text_element']" position="replace"> </xpath>
        <xpath expr="//t[@t-call='website.placeholder_header_brand']" position="replace">
            <div class="logo">
                <img src="/theme_coffee_shop/static/src/images/logo.png" alt=""/>
            </div>
        </xpath>
        <xpath expr="//t[@t-call='website.placeholder_header_call_to_action']" position="replace"> </xpath>
        </template>
</odoo>
